table {
	width: 100%;
	border-collapse: collapse;
	empty-cells: show;
}

td {
	text-align:center;
	fontFamily:"Helvetica, Verdana, Geneva, sans-serif";
	font-weight:normal;
	border: 1px solid #25383C;
	padding:10px;
	
}
tbody>tr>*:nth-last-child(1)~*{ width:10%}
tbody>tr>*:nth-last-child(2)~*{ width:45%}
tbody>tr>*:nth-last-child(3)~*{ width:45%}

tr:nth-child(even) {
	color:white;
}

tr:nth-child(odd) {
color:white;
}

th:nth-child(1)
{
	background-color:inherit;
	border:none;
}

th:nth-child(2)
{
	text-align:center;
	fontFamily:"Helvetica, Verdana, Geneva, sans-serif";
	font-weight:normal;
	font-size: 22px;
	background-color:#8d1919;
	color:white;
	padding:10px;
}

th:nth-child(3)
{
	text-align:center;
	fontFamily:"Helvetica, Verdana, Geneva, sans-serif"
	font-weight:normal;
	font-size: 22px;
	 background-color:#8d1919;
