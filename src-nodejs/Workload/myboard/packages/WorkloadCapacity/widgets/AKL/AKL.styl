/* basic positioning */
.legend { list-style: none; margin-top:70px;}
.legend li { float: left; margin-right: 20px; font-size:30px; fontFamily:"Helvetica, Verdana, Geneva, sans-serif";}
.legend span { border: 1px solid #ccc; float: left; width: 40px; height: 30px; margin: 10px; }
/* your colors */
.legend .schedHRS { background-color: rgba(100,100,100,1.0);}
.legend .cmplHRS { background-color: rgba(0,255,0,0.8); }
.legend .remHRS { background-color: rgba(255,255,255,0.8); }
.legend .incmplHRS { background-color: rgba(255,165,0,0.8); }
.legend .mouHRS { background-color: rgba(255,0,0,1); }


