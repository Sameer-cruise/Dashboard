table {
	width: 100%;
	border-collapse: collapse;
	border: 1px solid #000000 !important;
	empty-cells: show;
}

td ,tr{
	text-align:center;
	fontFamily:"Helvetica, Verdana, Geneva, sans-serif";
	font-weight:normal;
	/*border: 1px solid #25383C;*/
	border: 3px solid white;
}

tbody>tr>*:nth-last-child(1)~*{ width:10%}
tbody>tr>*:nth-last-child(2)~*{ width:45%}
tbody>tr>*:nth-last-child(3)~*{ width:45%}

tr:nth-child(even) {
	color:white;
}

tr:nth-child(odd) {
color:white;
}

