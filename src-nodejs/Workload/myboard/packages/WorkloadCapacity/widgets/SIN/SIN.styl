table {
	width: 100%;
	border-collapse: collapse;
	empty-cells: show;
}

td{
	text-align:center;
	fontFamily:"Helvetica, Verdana, Geneva, sans-serif";
	font-weight:normal;
	border: 1px solid #25383C;
	padding:10px;
	border: 3px solid white;
	
}

tbody>tr>*:nth-last-child(1)~*{ width:4%}
tbody>tr>*:nth-last-child(2)~*{ width:24%}
tbody>tr>*:nth-last-child(3)~*{ width:24%}
tbody>tr>*:nth-last-child(2)~*{ width:24%}
tbody>tr>*:nth-last-child(3)~*{ width:24%}

tr:nth-child(even) {
	color:white;
}

tr:nth-child(odd) {
color:white;
}

